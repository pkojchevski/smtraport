{
    "eqeqeq":    true,
    "undef":     true,
    "latedef":   true,
    "immed":     true,
    "browser":   true,
    "indent":    2,
    "maxdepth":  3,
    "curly":     true,
    "camelcase": true,
    "newcap":    true,
    "noempty":   true,
    "unused":    true,
    "trailing":  true,
    "multistr":  true,
    "quotmark":  "single",
    "maxcomplexity": 10,
    "maxstatements": 27,
    "maxlen":        135,
    "globals" : {
        "define"  : false,
        "require" : false,
        "console" : false,
        "module"  : false,
        "__dirname" : false,
        "process" : false
    }
}